<?xml version="1.0" encoding="utf-8"?>

    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/framelayoutajustes"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            app:layout_constraintStart_toStartOf="parent">

        </FrameLayout>

    <FrameLayout
        android:id="@+id/framelayoutmapa"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        app:layout_constraintStart_toStartOf="parent">
        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/btn5_ajustes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="300dp"
                android:layout_marginTop="50dp"
                android:minHeight="48dp"
                android:src="@drawable/ic_ajustes_black"
                tools:ignore="SpeakableTextPresentCheck" />


            <ImageView
                android:id="@+id/imgv5_manzanatutorial"
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="550dp"
                android:src="@drawable/img_upeliosagarra" />

            <ImageView
                android:id="@+id/imgv5_bocadillo"
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:layout_marginLeft="135dp"
                android:layout_marginTop="515dp"
                android:src="@drawable/img_bocadillo"

                />

            <in.codeshuffle.typewriterview.TypeWriterView
                android:id="@+id/txtv5_presentacionmapa"
                android:layout_width="165dp"
                android:layout_height="132dp"
                android:layout_marginLeft="170dp"
                android:layout_marginTop="560dp"
                android:textStyle="bold" />
        </fragment>
    </FrameLayout>
 </androidx.constraintlayout.widget.ConstraintLayout>



